{
  "manifest_version": 3,
  "name": "Excel Template Processor",
  "version": "1.0",
  "description": "Process Excel templates and crawl AliExpress items",
  "permissions": [
    "storage",
    "downloads",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["download.html"],
    "matches": ["<all_urls>"]
  }]
} 